<h1>Add Student</h1>

<div id="newWell" class="well">

  <%= form_tag "/contacts", method: :post do %>
  <div>
    <%= label_tag :first_name %>
    <%= text_field_tag :first_name %>
  </div>
  <br>
  <div>
    <%= label_tag :middle_name %>
    <%= text_field_tag :middle_name %>
  </div>
  <br>
  <div>
    <%= label_tag :last_name %>
    <%= text_field_tag :last_name %>
  </div>
  <br>
  <div>
    <%= label_tag :number %>
    <%= text_field_tag :number %>
  </div>
  <br>
  <div>
    <%= label_tag :"Greek God House" %>
    <%= text_field_tag :bio %>
  </div>
  <br>
  <div>
    <%= submit_tag "Add Student" %>
  </div>
<% end %>

</div>
