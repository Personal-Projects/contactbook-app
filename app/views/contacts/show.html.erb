<div id="newWell" class="well">

  <%= form_tag "/contacts", method: :post do %>
    <h4><%= @contact.first_name %></h4>
    <p>Number: <%= @contact.number %></p>
    <p>Greek God House: <%= @contact.bio %></p>
  <% end %>

  <br>
  &emsp;&emsp;<%= link_to "Edit", "/contacts/#{@contact.id}/edit", class: "btn btn-success" %>
  <%= link_to "Delete", "/contacts/#{@contact.id}", {method: :delete, class: "btn btn-danger" }%>

</div>