<h1>Login</h1>

<div id="newWell" class="well">

<%= form_tag "/login", method: :post do %>
  <div>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
  </div>
  <br>
  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <br>
  <div>
  <br>
    <%= submit_tag "Proceed" %>
  </div>
<% end %>

</div>

